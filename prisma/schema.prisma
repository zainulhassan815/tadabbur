generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Reflection {
  id        String   @id @default(cuid())
  content   String
  userId    String
  aayahId   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
